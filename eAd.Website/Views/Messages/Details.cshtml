@model eAd.DataAccess.Message

@{
    ViewBag.Title = "Details";
}

<h2>Details</h2>

<fieldset>
    <legend>Message</legend>

    <div class="display-label">Type</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.Type)
    </div>

    <div class="display-label">Text</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.Text)
    </div>

    <div class="display-label">Sent</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.Sent)
    </div>

    <div class="display-label">Station</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.Station.Name)
    </div>

    <div class="display-label">UserID</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.UserID)
    </div>

    <div class="display-label">Command</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.Command)
    </div>
</fieldset>
<p>
    @Html.ActionLink("Edit", "Edit", new { id=Model.MessageID }) |
