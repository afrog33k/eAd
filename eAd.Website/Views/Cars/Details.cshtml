@model eAd.DataAccess.Car

@{
    ViewBag.Title = "Details";
}

<h2>Details</h2>

<fieldset>
    <legend>Car</legend>

    <div class="display-label">Customer</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.Customer.Name)
    </div>

    <div class="display-label">Make</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.Make)
    </div>

    <div class="display-label">Model</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.Model)
    </div>

    <div class="display-label">License</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.License)
    </div>

    <div class="display-label">BatteryCharge</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.BatteryCharge)
    </div>

    <div class="display-label">LastRechargeDate</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.LastRechargeDate)
    </div>
</fieldset>
<p>
    @Html.ActionLink("Edit", "Edit", new { id=Model.CarID }) |
    @Html.ActionLink("Back to List", "Index")
</p>
