<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:Client="clr-namespace:Client"
    xmlns:ClientApp="clr-namespace:ClientApp"
    x:Class="ClientApp.Themes.x__ead_new_ead_desktopclient_ead_desktopclient_themes_generic_xaml">


    <Style TargetType="{x:Type ClientApp:Media}">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type ClientApp:Media}">
                    <Border Background="{TemplateBinding Background}"
                            BorderBrush="{TemplateBinding BorderBrush}"
                            BorderThickness="{TemplateBinding BorderThickness}">
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
</ResourceDictionary>
